{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/api.js';\nimport headerComp from '../components/Header.vue';\nimport footerComp from './Footer.vue';\nexport default {\n  name: 'landingPageComp',\n  data() {\n    return {\n      search: '',\n      movies: []\n    };\n  },\n  components: {\n    headerComp,\n    footerComp\n  },\n  methods: {\n    async searchMovies() {\n      if (this.search != '') {\n        await fetch(`https://www.omdbapi.com/?s=${this.search}&apikey=${api.apikey}`).then(response => response.json()).then(data => {\n          this.movies = data.Search;\n          this.search = '';\n        });\n      } else {\n        alert('Enter a movie name!');\n      }\n    }\n  },\n  mounted() {\n    let user = localStorage.getItem('user-info');\n    if (!user) {\n      return this.$router.push({\n        name: 'HomePage'\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAoCA,OAAOA,GAAE,MAAO,UAAU;AAC1B,OAAOC,UAAS,MAAO,0BAAyB;AAChD,OAAOC,UAAS,MAAO,cAAc;AACrC,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE;IAEZ;EACJ,CAAC;EACDC,UAAU,EAAE;IACRN,UAAU;IACVC;EACJ,CAAC;EACDM,OAAO,EAAE;IACN,MAAMC,YAAY,GAAG;MAChB,IAAI,IAAI,CAACJ,MAAK,IAAK,EAAE,EAAE;QACrB,MAAMK,KAAK,CAAE,8BAA6B,IAAI,CAACL,MAAO,WAAUL,GAAG,CAACW,MAAO,EAAC,EACrEC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,EAChCF,IAAI,CAACR,IAAG,IAAK;UACV,IAAI,CAACE,MAAK,GAAIF,IAAI,CAACW,MAAM;UACzB,IAAI,CAACV,MAAK,GAAI,EAAE;QACpB,CAAC,CAAC;MACV,CAAC,MAAI;QACDW,KAAK,CAAC,qBAAqB;MAC/B;IACJ;EACJ,CAAC;EACDC,OAAO,GAAE;IACL,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAC5C,IAAG,CAACF,IAAI,EAAC;MACL,OAAO,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC;QACrBnB,IAAI,EAAE;MACV,CAAC,CAAC;IACN;EACJ;AACJ","names":["api","headerComp","footerComp","name","data","search","movies","components","methods","searchMovies","fetch","apikey","then","response","json","Search","alert","mounted","user","localStorage","getItem","$router","push"],"sourceRoot":"","sources":["C:\\Workspace\\MovieApp\\src\\components\\LandingPage.vue"],"sourcesContent":["<template>\r\n<headerComp />\r\n<header>\r\n    <h2>MovieApp</h2>\r\n</header>\r\n\r\n<v-divider />\r\n\r\n<body>\r\n    <img src=\"../assets/movieLogo3.jpg\" alt=\"aMoviePoster\" />\r\n</body>\r\n\r\n<v-form @submit.prevent=\"searchMovies()\" class=\"searchBar\">\r\n    <v-responsive class=\"mx-auto\"  max-width=\"644\">\r\n        <v-text-field type=\"text\" placeholder=\"search for a movie...\" v-model=\"search\" />\r\n        <v-btn type=\"submit\" color=\"primary\" elevation=\"15\">submit</v-btn>\r\n    </v-responsive>\r\n</v-form>\r\n<v-divider />\r\n\r\n<v-container class=\"movieList\">\r\n    \r\n    <div class=\"movie\" v-for=\"(movie, i) in movies\" :key=\"i\">\r\n        <router-link :to=\"'/movie/' + movie.imdbID\" class=\"movie-router\">\r\n            <div class=\"searchResults\">\r\n                <img :src=\"movie.Poster\" alt=\"movie poster\" />\r\n                <h3>{{ movie.Title }}</h3>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</v-container>\r\n\r\n<footerComp />\r\n</template>\r\n\r\n<script>\r\nimport api from '@/api.js';\r\nimport headerComp from '../components/Header.vue'\r\nimport footerComp from './Footer.vue';\r\nexport default {\r\n    name: 'landingPageComp',\r\n    data() {\r\n        return {\r\n            search: '',\r\n            movies: []\r\n\r\n        }\r\n    },\r\n    components: {\r\n        headerComp,\r\n        footerComp\r\n    },\r\n    methods: {\r\n       async searchMovies() {\r\n            if (this.search != '') {\r\n              await fetch(`https://www.omdbapi.com/?s=${this.search}&apikey=${api.apikey}`)\r\n                    .then(response => response.json())\r\n                    .then(data => {\r\n                        this.movies = data.Search;\r\n                        this.search = '';\r\n                    });\r\n            }else{\r\n                alert('Enter a movie name!')\r\n            }\r\n        }\r\n    },\r\n    mounted(){\r\n        let user = localStorage.getItem('user-info');\r\n        if(!user){\r\n            return this.$router.push({\r\n                name: 'HomePage'\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n header h2 {\r\n     font-size: 40px;\r\n     font-weight: 600;\r\n     background-image: linear-gradient(to left, #553c9a, #b393d3);\r\n     color: transparent;\r\n     background-clip: text;\r\n     -webkit-background-clip: text;\r\n }\r\n\r\n img {\r\n     object-fit: contain;\r\n     width: 350px;\r\n     height: 350px;\r\n\r\n }\r\n\r\n \r\n\r\n .movieList {\r\n     display: flex;\r\n     flex-direction: row;\r\n     flex-wrap: wrap;\r\n     margin: 0px 8px;\r\n }\r\n\r\n\r\n\r\n #logout{\r\n    position: fixed;\r\n    right: 0;\r\n }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}